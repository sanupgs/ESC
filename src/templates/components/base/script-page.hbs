<script>


if($(window).width() < 980){
        // News
    var members_slider = new Swiper('.news_wrapper .swiper-container', {
        spaceBetween: 20,
        slidesPerView: 2,
        /*autoplay: {
            delay: 5000,
            disableOnInteraction: false,
        },*/
        navigation: {
            nextEl: '.news_wrapper .nav_.right_',
            prevEl: '.news_wrapper .nav_.left_',
        },
        breakpoints: {
            640: {
            slidesPerView: 1,
            },
            980: {
