            }
        }
    });
}

var parad_slider = new Swiper('.parad_slider .swiper-container', {
        spaceBetween: 30,
        slidesPerView: 3, 
        watchOverflow:true,
        /*autoplay: {
            delay: 5000,
            disableOnInteraction: false,
        },*/
        navigation: {
            nextEl: '.parad_slider .nav_.right_',
            prevEl: '.parad_slider .nav_.left_',
        },
        breakpoints: {
            980: {
                slidesPerView: 2,
            },
            767: {
                slidesPerView: 1,
            }
        },
        on: {
        init: function () {
        window.bLazy.revalidate();
            if ($(this)[0].snapGrid.length == 1) {
                $('.parad_slider .nav_').hide();
            }
        },
    }
});


var partner_slide = new Swiper('.partner_sec .swiper-container', {
